add wave -position insertpoint  \
sim:/eightbitcounter/u \
sim:/eightbitcounter/d \
sim:/eightbitcounter/clk \
sim:/eightbitcounter/reset \
sim:/eightbitcounter/a0 \
sim:/eightbitcounter/a1 \
sim:/eightbitcounter/a2 \
sim:/eightbitcounter/dd1 \
sim:/eightbitcounter/u1 \
sim:/eightbitcounter/q1 \
sim:/eightbitcounter/q0 \
sim:/eightbitcounter/q2 \
sim:/eightbitcounter/nq1 \
sim:/eightbitcounter/nq2 \
sim:/eightbitcounter/nq0 \
sim:/eightbitcounter/qq1 \
sim:/eightbitcounter/qq2 \
sim:/eightbitcounter/qq0 \
sim:/eightbitcounter/uu \
sim:/eightbitcounter/clki \
sim:/eightbitcounter/clkcount
force -freeze sim:/eightbitcounter/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/eightbitcounter/u 0 0
force -freeze sim:/eightbitcounter/d 0 0
force -freeze sim:/eightbitcounter/reset 1 0
run
force -freeze sim:/eightbitcounter/u 0 0
force -freeze sim:/eightbitcounter/d 0 0
force -freeze sim:/eightbitcounter/reset 0 0
run
